{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "magento/composer-dependency-version-audit-plugin": true,
            "magento/composer-root-update-plugin": true,
            "magento/magento-composer-installer": true,
            "magento/inventory-composer-installer": true,
            "cweagans/composer-patches": true
        }
    },
    "version": "2.4.4",
    "require": {
        "php": "^8.0|^8.1",
        "ext-json": "*",
        "ampersand/magento2-disable-stock-reservation": "^1.2",
        "apptrian/image-optimizer": "^2.2",
        "apptrian/minify": "^2.0",
        "bsscommerce/layered-navigation": "^1.2",
        "bsscommerce/quote-extension": "^1.2",
        "cweagans/composer-patches": "^1.7",
        "laminas/laminas-code": "~4.5.0",
        "landofcoder/module-next-gen-images": "^1.0",
        "magento-hackathon/module-eavcleaner-m2": "^1.5",
        "magento/composer-dependency-version-audit-plugin": "~0.1",
        "magento/composer-root-update-plugin": "^2.0",
        "magento/product-community-edition": "2.4.5",
        "mageplaza/magento-2-blog-extension": "^4.1",
        "mageplaza/magento-2-product-slider": "^4.0",
        "mageplaza/module-better-maintenance": "^4.0",
        "mageplaza/module-login-as-customer": "^4.0",
        "mageplaza/module-smtp": "^4.7",
        "mageplaza/module-webhook": "^4.1",
        "magestyapps/module-web-images": "^1.1",
        "markshust/magento2-module-disabletwofactorauth": "^1.1",
        "olegkoval/magento2-regenerate-url-rewrites": "^1.6",
        "opengento/module-gdpr": "^4.4",
        "rosell-dk/webp-convert": "^2.9",
        "symfony/polyfill-php80": "^1.23",
        "symfony/property-info": "^5.2",
        "symfony/string": "^5.1",
        "warbox/magento2-base-theme": "1.0.6",
        "warbox/windowspatch": "1.1.2",
        "webshopapps/module-matrixrate": "^20.2",
        "wyomind/massproductimport": "^10",
        "wyomind/watchlog": "^2.5"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ],
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "psr-4": {
            "Magento\\": "app/code/Magento/",
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/"
        }
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "^2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "dg/bypass-finals": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.8",
        "lusitanian/oauth": "^0.8",
        "magento/magento-coding-standard": "^33.0",
        "magento/magento2-functional-testing-framework": "^4.3.1",
        "pdepend/pdepend": "^2.10",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpmd/phpmd": "^2.8.0",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9",
        "sebastian/comparator": "<=4.0.6",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3",
        "symfony/finder": "^5.4",
        "symfony/process": "<=v5.4.23"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\PhpStan\\": "dev/tests/static/framework/Magento/PhpStan/",
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "bsscommerce": {
            "type": "composer",
            "url": "https://composer.bsscommerce.com/community"
        },
        "wyomind": {
            "type": "composer",
            "url": "https://devops_warboxcreative_co_uk:JoYEr6kM@repo.wyomind.com"
        },
        "0": {
            "type": "composer",
            "url": "https://repo.mageplaza.com"
        },
        "1": {
            "type": "composer",
            "url": "https://repo.magento.com/",
	    "exclude": ["mageplaza/magento-2-blog-extension"]
        },
        "2": {
            "url": "https://github.com/WarboxCreative/magento-windowspatch.git",
            "type": "vcs"
        },
        "3": {
            "url": "https://github.com/WarboxCreative/magento2-base-theme.git",
            "type": "vcs"
        },
        "4": {
            "url": "https://github.com/WarboxCreative/widget-elements.git",
            "type": "vcs"
        }
    },
    "extra": {
        "magento-force": "override",
        "composer-exit-on-patch-failure": true
    }
}
