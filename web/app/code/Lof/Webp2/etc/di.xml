<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Lof\NextGenImages\Convertor\ConvertorListing">
        <arguments>
            <argument name="convertors" xsi:type="array">
                <item name="webp2" xsi:type="object">Lof\Webp2\Convertor\Convertor</item>
            </argument>
        </arguments>
    </type>

    <virtualType name="Lof\Webp2\VirtualType\Block\Adminhtml\System\Config\ModuleVersion" type="Lof\NextGenImages\Block\Adminhtml\System\Config\ModuleVersion">
        <arguments>
            <argument name="moduleName" xsi:type="string">Lof_Webp2</argument>
        </arguments>
    </virtualType>
</config>
